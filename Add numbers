import java.util.Scanner;
 
class AddNumbers
{
   public static void main(String args[])
   {
      int a, b, c;
 
      System.out.println("Enter two integers to calculate their sum : \n");
      Scanner sc = new Scanner(System.in);
     
      a = sc.nextInt();
      b = sc.nextInt();
      c = a + b;
     
      System.out.println("Sum of the integers = " + c);
   }
}
