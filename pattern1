import java.util.Scanner;

class square
{
public static void main(String args[])
    {
        int n;
        Scanner Sq=new Scanner(System.in);
        System.out.print("Enter the number :");
        n=Sq.nextInt();

        int i=1;
        while(i<=n)
        { 
            int j=1;
            while(j<=n)
            {
                System.out.print("*");
                j++;
            }
            i++;
            System.out.println();
        }
    }
}
